{
  "name": "js-primer",
  "private": true,
  "license": "MIT",
  "version": "1.0.0",
  "description": "JavaScriptの入門本",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "gitbook install",
    "start": "gitbook serve",
    "build": "gitbook build",
    "eslint": "eslint source/",
    "eslint:fix": "eslint --fix source/",
    "textlint": "summary-to-path ./source/README.md | xargs textlint -f pretty-error",
    "textlint:cached": "summary-to-path ./source/README.md | file-cache-cli | xargs textlint -f pretty-error",
    "textlint:fix": "summary-to-path ./source/README.md | xargs textlint --fix",
    "lint": "npm-run-all --parallel textlint eslint",
    "lint:fix": "npm-run-all --parallel textlint:fix eslint:fix",
    "test:example": "mocha test/",
    "test:basic": "mocha './source/basic/**/*-test.js'",
    "test:ajaxapp": "mocha './source/ajaxapp/!(node_modules)**/*-test.js'",
    "test": "npm-run-all -l clean --parallel test:* lint",
    "clean": "file-cache-cli-clean",
    "travis": "npm-run-all -l --parallel test build"
  },
  "keywords": [
    "gitbook",
    "textlint"
  ],
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jsdoc-to-assert": "^2.0.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.7.2",
    "eslint": "^3.0.0",
    "file-cache-cli": "^1.0.1",
    "front-matter": "^2.1.0",
    "gitbook-cli": "^2.1.2",
    "gitbook-plugin-canonical-link": "^2.0.2",
    "gitbook-plugin-edit-link": "^2.0.2",
    "gitbook-plugin-ga": "^1.0.1",
    "gitbook-plugin-include-codeblock": "^2.0.0",
    "gitbook-plugin-js-console": "^1.3.2",
    "gitbook-summary-to-path": "^1.0.1",
    "globby": "^6.0.0",
    "mocha": "^3.0.2",
    "npm-run-all": "^2.3.0",
    "power-assert": "^1.3.1",
    "power-doctest": "^1.1.1",
    "remark": "^5.0.1",
    "strict-eval": "^1.0.1",
    "text-testing-mocha": "^0.2.0",
    "textlint": "^7.0.1",
    "textlint-filter-rule-comments": "^1.2.1",
    "textlint-filter-rule-whitelist": "^1.0.2",
    "textlint-rule-eslint": "^1.1.1",
    "textlint-rule-preset-ja-technical-writing": "^0.1.1",
    "textlint-rule-prh": "^3.0.1",
    "unist-util-select": "^1.5.0"
  },
  "dependencies": {}
}
